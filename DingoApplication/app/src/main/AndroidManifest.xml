<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leon.dingoapplication" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:theme="@style/dingoTheme" >
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.CollectionActivity"
            android:label="@string/collection" >
        </activity>
        <activity
            android:name=".Activities.CustomerActivity"
            android:label="@string/title_activity_customer" >
        </activity>
        <activity
            android:name=".Activities.EatDrinkActivity"
            android:label="@string/title_activity_eat_drink" >
        </activity>
    </application>

</manifest>
